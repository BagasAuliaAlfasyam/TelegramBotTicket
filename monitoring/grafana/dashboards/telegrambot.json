{
  "uid": "telegrambot-mytech",
  "title": "TelegramBotMyTech",
  "tags": ["telegrambot", "ml", "mytech"],
  "timezone": "Asia/Jakarta",
  "refresh": "30s",
  "schemaVersion": 39,
  "version": 1,
  "time": { "from": "now-6h", "to": "now" },
  "panels": [
    {
      "id": 1,
      "title": "Total Predictions",
      "type": "stat",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "datasource": { "uid": "prometheus" },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background", "graphMode": "none", "orientation": "auto" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "value": 0, "color": "blue" }] }, "unit": "short", "decimals": 0 } },
      "targets": [{ "expr": "sum(increase(ticket_predictions_total[$__range]))", "legendFormat": "Total", "refId": "A" }]
    },
    {
      "id": 2,
      "title": "AUTO Rate",
      "type": "stat",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "datasource": { "uid": "prometheus" },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background", "graphMode": "none", "orientation": "auto" },
      "fieldConfig": { "defaults": { "unit": "percent", "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "value": 0, "color": "red" }, { "value": 60, "color": "yellow" }, { "value": 80, "color": "green" }] } } },
      "targets": [{ "expr": "sum(increase(ticket_predictions_total{status=\"AUTO\"}[$__range])) / sum(increase(ticket_predictions_total[$__range])) * 100", "legendFormat": "AUTO %", "refId": "A" }]
    },
    {
      "id": 3,
      "title": "Avg Confidence (5m)",
      "type": "stat",
      "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
      "datasource": { "uid": "prometheus" },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background", "graphMode": "none" },
      "fieldConfig": { "defaults": { "unit": "percent", "decimals": 1, "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "value": 0, "color": "red" }, { "value": 70, "color": "yellow" }, { "value": 80, "color": "green" }] } } },
      "targets": [{ "expr": "sum(rate(ticket_prediction_confidence_sum[5m])) / sum(rate(ticket_prediction_confidence_count[5m])) * 100", "legendFormat": "Avg Confidence", "refId": "A" }]
    },
    {
      "id": 4,
      "title": "Gemini Calls (1h)",
      "type": "stat",
      "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
      "datasource": { "uid": "prometheus" },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background", "graphMode": "none" },
      "fieldConfig": { "defaults": { "unit": "short", "decimals": 0, "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "value": 0, "color": "green" }, { "value": 20, "color": "yellow" }, { "value": 50, "color": "red" }] } } },
      "targets": [{ "expr": "sum(increase(ticket_gemini_calls_total[1h]))", "legendFormat": "Gemini/hr", "refId": "A" }]
    },
    {
      "id": 5,
      "title": "Prediction Rate (by Status)",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
      "datasource": { "uid": "prometheus" },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "fieldConfig": {
        "defaults": { "unit": "reqps", "custom": { "lineWidth": 2, "fillOpacity": 10 } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "AUTO" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "green" }}] },
          { "matcher": { "id": "byName", "options": "REVIEW" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" }}] }
        ]
      },
      "targets": [{ "expr": "sum by (status) (rate(ticket_predictions_total[5m]))", "legendFormat": "{{status}}", "refId": "A" }]
    },
    {
      "id": 6,
      "title": "HTTP Request Latency â€” prediction-api",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 8 },
      "datasource": { "uid": "prometheus" },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "fieldConfig": { "defaults": { "unit": "s", "custom": { "lineWidth": 2 } } },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum by (le)(rate(http_request_duration_seconds_bucket{job=\"prediction-api\"}[5m])))", "legendFormat": "p50", "refId": "A" },
        { "expr": "histogram_quantile(0.95, sum by (le)(rate(http_request_duration_seconds_bucket{job=\"prediction-api\"}[5m])))", "legendFormat": "p95", "refId": "B" },
        { "expr": "histogram_quantile(0.99, sum by (le)(rate(http_request_duration_seconds_bucket{job=\"prediction-api\"}[5m])))", "legendFormat": "p99", "refId": "C" }
      ]
    },
    {
      "id": 7,
      "title": "Predictions by Label (Top Categories)",
      "type": "bargauge",
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 8 },
      "datasource": { "uid": "prometheus" },
      "options": { "orientation": "horizontal", "reduceOptions": { "calcs": ["lastNotNull"] }, "displayMode": "gradient", "valueMode": "color", "showUnfilled": true },
      "fieldConfig": { "defaults": { "unit": "short", "decimals": 0, "color": { "mode": "palette-classic" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] } } },
      "targets": [{ "expr": "topk(10, sum by (label)(increase(ticket_predictions_total[$__range])))", "legendFormat": "{{label}}", "refId": "A" }]
    },
    {
      "id": 8,
      "title": "HTTP Error Rate (5xx)",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 12, "w": 12, "h": 8 },
      "datasource": { "uid": "prometheus" },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "fieldConfig": {
        "defaults": { "unit": "reqps", "custom": { "lineWidth": 2, "fillOpacity": 15 }, "color": { "mode": "fixed", "fixedColor": "red" } }
      },
      "targets": [
        { "expr": "sum by (job, status)(rate(http_requests_total{status=~\"5..\"}[5m]))", "legendFormat": "{{job}} {{status}}", "refId": "A" }
      ]
    },
    {
      "id": 9,
      "title": "Confidence Score Distribution",
      "type": "bargauge",
      "gridPos": { "x": 0, "y": 20, "w": 12, "h": 8 },
      "datasource": { "uid": "prometheus" },
      "options": { "orientation": "horizontal", "reduceOptions": { "calcs": ["lastNotNull"] }, "displayMode": "gradient", "valueMode": "color", "showUnfilled": true },
      "fieldConfig": { "defaults": { "unit": "percentunit", "min": 0, "max": 1, "color": { "mode": "continuous-GrYlRd" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] } } },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum by (le)(rate(ticket_prediction_confidence_bucket[$__rate_interval])))", "legendFormat": "p50", "refId": "A" },
        { "expr": "histogram_quantile(0.75, sum by (le)(rate(ticket_prediction_confidence_bucket[$__rate_interval])))", "legendFormat": "p75", "refId": "B" },
        { "expr": "histogram_quantile(0.90, sum by (le)(rate(ticket_prediction_confidence_bucket[$__rate_interval])))", "legendFormat": "p90", "refId": "C" },
        { "expr": "histogram_quantile(0.95, sum by (le)(rate(ticket_prediction_confidence_bucket[$__rate_interval])))", "legendFormat": "p95", "refId": "D" }
      ]
    },
    {
      "id": 10,
      "title": "Gemini Cascade Rate",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 20, "w": 12, "h": 8 },
      "datasource": { "uid": "prometheus" },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "fieldConfig": { "defaults": { "unit": "reqps", "custom": { "lineWidth": 2, "fillOpacity": 10 } } },
      "targets": [
        { "expr": "sum by (outcome)(rate(ticket_gemini_calls_total[5m]))", "legendFormat": "{{outcome}}", "refId": "A" }
      ]
    },
    {
      "id": 11,
      "title": "Application Logs",
      "type": "logs",
      "gridPos": { "x": 0, "y": 28, "w": 24, "h": 10 },
      "datasource": { "uid": "loki" },
      "options": { "showTime": true, "showLabels": true, "showCommonLabels": false, "wrapLogMessage": true, "sortOrder": "Descending", "dedupStrategy": "none", "enableLogDetails": true },
      "targets": [{ "expr": "{service=~\"prediction-api|data-api|collector-bot|admin-bot\"}", "legendFormat": "", "refId": "A" }]
    },
    {
      "id": 12,
      "title": "Error Logs Only",
      "type": "logs",
      "gridPos": { "x": 0, "y": 38, "w": 24, "h": 8 },
      "datasource": { "uid": "loki" },
      "options": { "showTime": true, "showLabels": true, "wrapLogMessage": true, "sortOrder": "Descending", "dedupStrategy": "none", "enableLogDetails": true },
      "targets": [{ "expr": "{service=~\"prediction-api|data-api|collector-bot|admin-bot\"} | level=\"ERROR\"", "legendFormat": "", "refId": "A" }]
    }
  ]
}
